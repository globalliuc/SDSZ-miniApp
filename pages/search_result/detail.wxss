.table {
  border: 0px solid darkgray;
}
.tr {
  font-size: 13px;
  display: flex;
  width: 99%;
  justify-content: center;
  height: 2rem;
  align-items: center;
  border: 1px solid black;
}
.td {
    font-size: 13px;
    width:50%;
    justify-content: center;
    text-align: center;
}

.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 50%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  border: 1px solid black;
  display: flex;
  height: 2rem;
  align-items: center;
}


